//INPUT:
//Program to read 2 string and print both are ANAGRAM or NOT ANAGRAM
#include<stdio.h>
#include<string.h>
void main()
{
  int value;
  char s1[30],s2[30];
  printf("enter string 1:");
  gets(s1);
  printf("enter string 2:");
  gets(s2);
  value=strcmp(s1,s2);
  if(value==0)
    printf("same");
  else
    printf("not same");
}
//OUTPUT:
//enter string 1:below
//enter string 2:below
//same
